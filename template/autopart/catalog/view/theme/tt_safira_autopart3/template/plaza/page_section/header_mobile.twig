<!DOCTYPE html>
<!--[if IE]><![endif]-->
<!--[if IE 8 ]><html dir="{{ direction }}" lang="{{ lang }}" class="ie8"><![endif]-->
<!--[if IE 9 ]><html dir="{{ direction }}" lang="{{ lang }}" class="ie9"><![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<html dir="{{ direction }}" lang="{{ lang }}">
<!--<![endif]-->
<head>
<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<title>{{ title }}</title>
<base href="{{ base }}" />
{% if description %}
	<meta name="description" content="{{ description }}" />
{% endif %}
{% if keywords %}
	<meta name="keywords" content="{{ keywords }}" />
{% endif %}
<link href="catalog/view/javascript/bootstrap/css/bootstrap.min.css" rel="stylesheet" media="screen" />
{% for style in styles %}
<link href="{{ style.href }}" type="text/css" rel="{{ style.rel }}" media="{{ style.media }}" />
{% endfor %}
<link href="catalog/view/javascript/jquery/swiper/css/swiper.min.css" rel="stylesheet" type="text/css" />
<!-- icon font -->
<link href="catalog/view/javascript/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
<link href="catalog/view/javascript/ionicons/css/ionicons.css" rel="stylesheet" type="text/css" />
<!-- end icon font -->
{% if minify_css %}
<link href="{{ minify_css_url }}" rel="stylesheet" media="screen" />
{% endif %}
{% if not minify_css %}
<link href="catalog/view/theme/tt_safira_autopart3/stylesheet/stylesheet.css" rel="stylesheet">
<link href="catalog/view/theme/tt_safira_autopart3/stylesheet/plaza/header/header{{ header_layout }}.css" rel="stylesheet">
<link href="catalog/view/theme/tt_safira_autopart3/stylesheet/plaza/theme.css" rel="stylesheet">
<!-- Quick view -->
{% if use_quick_view %}
<link href="catalog/view/theme/tt_safira_autopart3/stylesheet/plaza/quickview/quickview.css" rel="stylesheet" type="text/css" />
{% endif %}
<!-- Advance -->
{% if custom_css %}
<style>
{{ custom_css }}
</style>
{% endif %}
{% endif %}
{% if minify_js %}
<script src="{{ minify_js_url }}" ></script>
{% endif %}
{% if not minify_js %}
<script src="catalog/view/javascript/jquery/jquery-2.1.1.min.js" ></script>
<!-- Bootstrap Js -->
<script src="catalog/view/javascript/bootstrap/js/bootstrap.min.js" ></script>
<script src="catalog/view/javascript/jquery/swiper/js/swiper.min.js" ></script>
{% if lazy_load %}
<script src="catalog/view/javascript/plaza/lazyload/lazysizes.min.js"></script>
{% endif %}
{% if use_quick_view %}
<script src="catalog/view/javascript/plaza/cloudzoom/cloud-zoom.1.0.2.min.js" ></script>
<script src="catalog/view/javascript/plaza/cloudzoom/zoom.js" ></script>
<script src="catalog/view/javascript/plaza/quickview/quickview.js" ></script>
{% endif %}
{% for script in scripts %}
<script src="{{ script }}" ></script>
{% endfor %}
<script src="catalog/view/javascript/common.js" ></script>
{% if custom_js %}
<script>
{{ custom_js }}
</script>
{% endif %}
{% endif %}
{% for link in links %}
<link href="{{ link.href }}" rel="{{ link.rel }}" />
{% endfor %}
{% for analytic in analytics %}
{{ analytic }}
{% endfor %}
</head>
<body class="{{ class }} mobile-layout">
<input type="hidden" id="hd-check-lazy" value="{% if lazy_load %}1{% else %}0{% endif %}" />
<input type="hidden" id="hd-check-scroll-top" value="{% if scroll_top %}1{% else %}0{% endif %}" />
<input type="hidden" id="hd-check-sticky-header-mobile" value="{% if sticky_header %}1{% else %}0{% endif %}" />
<div class="wrapper">
{% if scroll_top %}
    <div id="back-top"><i class="ion-chevron-up"></i></div>
{% endif %}
<div id="header">
	<header class="scroll-fix ">
		<div class="header-middle">
			<div class="container">
				<div class="box-inner">
					<div class="box-inner-inner">
						<div class="col-logo">
							{{ position3 }}				
							<div id="logo">
							{% if logo %}<a href="{{ home }}"><img src="{{ logo }}" title="{{ name }}" alt="{{ name }}" class="img-responsive" /></a>{% else %}
							  <h1><a href="{{ home }}">{{ name }}</a></h1>
							  {% endif %}
							</div>
						</div>
						<div class="col-cart">
							<div class="inner">
								<div class="box-setting btn-group">
									<button class="dropdown-toggle" data-toggle="dropdown"><i class="ion-android-close" aria-hidden="true"></i></button>
									<ul class="dropdown-menu">
										<li class="currency">
										{% if header_currency %}
										  {{ currency }}
										{% endif %}
										</li>
										<li class="language">
										{{ language }}
										</li>
										<li id="top-links">
										  <ul class="list-inline">
											<li class="dropdown"><a href="{{ account }}" title="{{ text_account }}" ><i class="fa fa-user"></i><span>{{ text_account }}</span><i class="icon-right ion-chevron-down"></i></a>
											  <ul class="dropdown-menu-right pt-account">
												{% if logged %}
												<li><a href="{{ account }}">{{ text_account }}</a></li>
												<li><a href="{{ order }}">{{ text_order }}</a></li>
												<li><a href="{{ transaction }}">{{ text_transaction }}</a></li>
												<li><a href="{{ download }}">{{ text_download }}</a></li>
												<li><a id="pt-logout-link" href="{{ logout }}">{{ text_logout }}</a></li>
												{% else %}
												<li><a id="pt-register-link" href="{{ register }}">{{ text_register }}</a></li>
												<li><a id="pt-login-link" href="{{ login }}">{{ text_login }}</a></li>
												{% endif %}
											  </ul>
											</li>
										  </ul>
										</li>
										<li class="header-email"><p><span>{{ email }}</span></p></li>
										<li class="header-phone">
											<div class="inner">
												<div class="inner-inner">
													<p>{{ text_call_us }}</p>
													<span>{{ telephone }}</span>
												</div>
											</div>
										</li>
										
										<!--li><a href="{{ shopping_cart }}" title="{{ text_shopping_cart }}"><i class="fa fa-shopping-cart"></i> <span>{{ text_shopping_cart }}</span></a></li>
										<li><a href="{{ checkout }}" title="{{ text_checkout }}"><i class="fa fa-share"></i> <span>{{ text_checkout }}</span></a></li>
										<li><a href="{{ contact }}" ><span>{{ text_hot_line }} {{ telephone }}</span></a></li-->
									</ul>
								</div>
								<a href="{{ wishlist }}" id="wishlist-total" ><span>{{ text_wishlist }}</span></a>
								{% if header_cart %}{{ cart }}{% endif %}
							</div>
						</div>
					</div>
					<div class="col-search">{{ search }}</div>
				</div>
			</div>
		</div>
		
	</header>
</div>